{
  "name": "@whoersir/gomoku-server",
  "version": "1.0.3",
  "description": "Gomoku LAN game server",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoersir/Gomoku.git"
  },
  "author": "whoersir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whoersir/Gomoku/issues"
  },
  "homepage": "https://github.com/whoersir/Gomoku#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "build:prod": "tsc",
    "publish": "npm publish",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "music-metadata": "^8.1.3",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
